<div class="index">
  <h1>My movie lists</h1>
</div>
<div class="row justify-content-between">
  <% @lists.each do |l| %>
    <div class="col-3 m-5">
      <%= link_to list_path(l), class: "text-decoration-none" do %>
        <div class="cards">
          <p><%= l.name %></p>
        </div>
      <% end %>
    </div>
  <% end %>
</div>
