<div class="index">
  <h1>My movie lists</h1>
</div>
<div class="row">
  <% @lists.each do |l| %>
    <div class="col-3 m-5">
      <%= link_to list_path(l), class: "text-decoration-none" do %>
        <div class="cards" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path l.photo.key, height: 300, crop: :fill %>')">
          <p><%= l.name %></p>
        </div>
      <% end %>
    </div>
  <% end %>
</div>
